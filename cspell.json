{
  "version": "0.2",
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "language": "en",
  "dictionaries": ["vgv_allowed", "vgv_forbidden"],
  "dictionaryDefinitions": [
    {
      "name": "vgv_allowed",
      "path": "https://raw.githubusercontent.com/verygoodopensource/very_good_dictionaries/main/allowed.txt",
      "description": "Allowed VGV Spellings"
    },
    {
      "name": "vgv_forbidden",
      "path": "https://raw.githubusercontent.com/verygoodopensource/very_good_dictionaries/main/forbidden.txt",
      "description": "Forbidden VGV Spellings"
    }
  ],
  "useGitignore": true,
  "words": [
    "pubspec",
    "mocktail",
    "genhtml",
    "appbundle",
    "xcworkspace",
    "xcassets",
    "subosito",
    "choco",
    "lefthook",
    "cspell",
    "publishability",
    "Contador",
    "localizable",
    "Nueva",
    "Cadena",
    "Texto",
    "mostrado",
    "página",
    "writeln",
    "unvalidated",
    "nbsp",
    "Quản",
    "trạng",
    "thái",
    "phương",
    "thức",
    "tăng",
    "giảm",
    "Tăng",
    "hình",
    "trường",
    "Không",
    "thấy",
    "đúng",
    "Kiểm",
    "Nhat",
    "Nguoi",
    "Quan",
    "Hướng",
    "Tổng",
    "dụng",
    "phát",
    "triển",
    "nhật",
    "kiếm",
    "quản",
    "dịch",
    "người",
    "dùng",
    "thống",
    "Dụng",
    "Tiêu",
    "Thêm",
    "tính",
    "năng",
    "quan",
    "Kiến",
    "trúc",
    "Khắc",
    "phục"
  ],
  "ignorePaths": [
    "build/**",
    "coverage/**",
    ".dart_tool/**",
    "node_modules/**",
    "**/*.lock",
    "**/*.g.dart",
    "**/generated/**"
  ],
  "overrides": [
    {
      "filename": "**/*.md",
      "dictionaries": ["markdown"]
    },
    {
      "filename": "**/*.dart",
      "dictionaries": ["dart", "flutter"]
    }
  ]
}
