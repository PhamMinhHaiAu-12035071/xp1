# Staging Environment Configuration
# Copy this file to staging.env and fill in your actual values

# Base API URL for backend communication
API_URL=https://staging-api.yourdomain.com/api

# Application name used for display and identification
APP_NAME=Xp1 Staging

# Environment identifier, used for debugging and logging
ENVIRONMENT_NAME=staging

# Debug mode flag for development features (true/false)
IS_DEBUG_MODE=false

# API timeout duration in milliseconds
API_TIMEOUT_MS=20000
